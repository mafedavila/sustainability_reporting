          "editorMode": "code",
          "format": "table",
          "rawQuery": true,
          "rawSql": "SELECT \n    'Renewables' AS category,\n    SUM(\n        es.mwh * (\n            CASE \n                WHEN es.sourcename IN ('Biomass', 'Hydropower', 'Wind Offshore', 'Wind Onshore', 'Photovoltaic', 'Other Renewables') \n                THEN 1 \n                ELSE 0 \n            END\n        )\n    ) / (SELECT SUM(mwh) FROM (\n        SELECT \n            SUM(biomasse_mwh) AS mwh\n        FROM \n            energy_smard\n        UNION ALL\n        SELECT \n            SUM(wasserkraft_mwh) AS mwh\n        FROM \n            energy_smard\n        UNION ALL\n        SELECT \n            SUM(wind_offshore_mwh) AS mwh\n        FROM \n            energy_smard\n        UNION ALL\n        SELECT \n            SUM(wind_onshore_mwh) AS mwh\n        FROM \n            energy_smard\n        UNION ALL\n        SELECT \n            SUM(photovoltaik_mwh) AS mwh\n        FROM \n            energy_smard\n        UNION ALL\n        SELECT \n            SUM(sonstige_erneuerbare_mwh) AS mwh\n        FROM \n            energy_smard\n        UNION ALL\n        SELECT \n            SUM(kernenergie_mwh) AS mwh\n        FROM \n            energy_smard\n        UNION ALL\n        SELECT \n            SUM(braunkohle_mwh) AS mwh\n        FROM \n            energy_smard\n        UNION ALL\n        SELECT \n            SUM(steinkohle_mwh) AS mwh\n        FROM \n            energy_smard\n        UNION ALL\n        SELECT \n            SUM(erdgas_mwh) AS mwh\n        FROM \n            energy_smard\n        UNION ALL\n        SELECT \n            SUM(pumpspeicher_mwh) AS mwh\n        FROM \n            energy_smard\n        UNION ALL\n        SELECT \n            SUM(sonstige_konventionelle_mwh) AS mwh\n        FROM \n            energy_smard\n    ) AS total_energy) * 100 AS percentage\nFROM \n    (\n        SELECT 'Biomass' AS sourcename, SUM(biomasse_mwh) AS mwh FROM energy_smard\n        UNION ALL\n        SELECT 'Hydropower' AS sourcename, SUM(wasserkraft_mwh) AS mwh FROM energy_smard\n        UNION ALL\n        SELECT 'Wind Offshore' AS sourcename, SUM(wind_offshore_mwh) AS mwh FROM energy_smard\n        UNION ALL\n        SELECT 'Wind Onshore' AS sourcename, SUM(wind_onshore_mwh) AS mwh FROM energy_smard\n        UNION ALL\n        SELECT 'Photovoltaic' AS sourcename, SUM(photovoltaik_mwh) AS mwh FROM energy_smard\n        UNION ALL\n        SELECT 'Other Renewables' AS sourcename, SUM(sonstige_erneuerbare_mwh) AS mwh FROM energy_smard\n        UNION ALL\n        SELECT 'Nuclear Energy' AS sourcename, SUM(kernenergie_mwh) AS mwh FROM energy_smard\n        UNION ALL\n        SELECT 'Lignite' AS sourcename, SUM(braunkohle_mwh) AS mwh FROM energy_smard\n        UNION ALL\n        SELECT 'Hard Coal' AS sourcename, SUM(steinkohle_mwh) AS mwh FROM energy_smard\n        UNION ALL\n        SELECT 'Natural Gas' AS sourcename, SUM(erdgas_mwh) AS mwh FROM energy_smard\n        UNION ALL\n        SELECT 'Pumped Storage' AS sourcename, SUM(pumpspeicher_mwh) AS mwh FROM energy_smard\n        UNION ALL\n        SELECT 'Other Conventional' AS sourcename, SUM(sonstige_konventionelle_mwh) AS mwh FROM energy_smard\n    ) es\nUNION ALL\nSELECT \n    'Conventional' AS category,\n    SUM(\n        es.mwh * (\n            CASE \n                WHEN es.sourcename NOT IN ('Biomass', 'Hydropower', 'Wind Offshore', 'Wind Onshore', 'Photovoltaic', 'Other Renewables') \n                THEN 1 \n                ELSE 0 \n            END\n        )\n    ) / (SELECT SUM(mwh) FROM (\n        SELECT \n            SUM(biomasse_mwh) AS mwh\n        FROM \n            energy_smard\n        UNION ALL\n        SELECT \n            SUM(wasserkraft_mwh) AS mwh\n        FROM \n            energy_smard\n        UNION ALL\n        SELECT \n            SUM(wind_offshore_mwh) AS mwh\n        FROM \n            energy_smard\n        UNION ALL\n        SELECT \n            SUM(wind_onshore_mwh) AS mwh\n        FROM \n            energy_smard\n        UNION ALL\n        SELECT \n            SUM(photovoltaik_mwh) AS mwh\n        FROM \n            energy_smard\n        UNION ALL\n        SELECT \n            SUM(sonstige_erneuerbare_mwh) AS mwh\n        FROM \n            energy_smard\n        UNION ALL\n        SELECT \n            SUM(kernenergie_mwh) AS mwh\n        FROM \n            energy_smard\n        UNION ALL\n        SELECT \n            SUM(braunkohle_mwh) AS mwh\n        FROM \n            energy_smard\n        UNION ALL\n        SELECT \n            SUM(steinkohle_mwh) AS mwh\n        FROM \n            energy_smard\n        UNION ALL\n        SELECT \n            SUM(erdgas_mwh) AS mwh\n        FROM \n            energy_smard\n        UNION ALL\n        SELECT \n            SUM(pumpspeicher_mwh) AS mwh\n        FROM \n            energy_smard\n        UNION ALL\n        SELECT \n            SUM(sonstige_konventionelle_mwh) AS mwh\n        FROM \n            energy_smard\n    ) AS total_energy) * 100 AS percentage\nFROM \n    (\n        SELECT 'Biomass' AS sourcename, SUM(biomasse_mwh) AS mwh FROM energy_smard\n        UNION ALL\n        SELECT 'Hydropower' AS sourcename, SUM(wasserkraft_mwh) AS mwh FROM energy_smard\n        UNION ALL\n        SELECT 'Wind Offshore' AS sourcename, SUM(wind_offshore_mwh) AS mwh FROM energy_smard\n        UNION ALL\n        SELECT 'Wind Onshore' AS sourcename, SUM(wind_onshore_mwh) AS mwh FROM energy_smard\n        UNION ALL\n        SELECT 'Photovoltaic' AS sourcename, SUM(photovoltaik_mwh) AS mwh FROM energy_smard\n        UNION ALL\n        SELECT 'Other Renewables' AS sourcename, SUM(sonstige_erneuerbare_mwh) AS mwh FROM energy_smard\n        UNION ALL\n        SELECT 'Nuclear Energy' AS sourcename, SUM(kernenergie_mwh) AS mwh FROM energy_smard\n        UNION ALL\n        SELECT 'Lignite' AS sourcename, SUM(braunkohle_mwh) AS mwh FROM energy_smard\n        UNION ALL\n        SELECT 'Hard Coal' AS sourcename, SUM(steinkohle_mwh) AS mwh FROM energy_smard\n        UNION ALL\n        SELECT 'Natural Gas' AS sourcename, SUM(erdgas_mwh) AS mwh FROM energy_smard\n        UNION ALL\n        SELECT 'Pumped Storage' AS sourcename, SUM(pumpspeicher_mwh) AS mwh FROM energy_smard\n        UNION ALL\n        SELECT 'Other Conventional' AS sourcename, SUM(sonstige_konventionelle_mwh) AS mwh FROM energy_smard\n    ) es;\n",
