          "rawQuery": true,
          "rawSql": "WITH total_emissions AS (\n    SELECT \n        SUM(energyemission) AS total_energy_emissions,\n        SUM(materialemission) AS total_material_emissions,\n        SUM(transportemission) AS total_transport_emissions,\n        SUM(energyemission + materialemission + transportemission) AS total_emissions\n    FROM \n        product_footprint\n)\nSELECT \n    'Energy' AS category,\n    (te.total_energy_emissions / te.total_emissions) * 100 AS percentage\nFROM \n    total_emissions te\n\nUNION ALL\n\nSELECT \n    'Material' AS category,\n    (te.total_material_emissions / te.total_emissions) * 100 AS percentage\nFROM \n    total_emissions te\n\nUNION ALL\n\nSELECT \n    'Transport' AS category,\n    (te.total_transport_emissions / te.total_emissions) * 100 AS percentage\nFROM \n    total_emissions te;\n",
